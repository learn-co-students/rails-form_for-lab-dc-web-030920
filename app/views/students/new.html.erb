<h1>Student Form</h1>

                <%= form_for(@student) do |f|%>

                <label for="first name">First Name: </label><br>

                <%=f.text_field :first_name%><br>

                <label for="last name">Last Name: </label><br>

                <%=f.text_field  :last_name%><br>

                <%=f.submit value="Create Student"%>

        <%end%>


