<h1>School Class Form</h1>

        <%=form_for @school_class do |f|%>

        <label for="title">Title: </label>

        <%=f.text_field :title%>

        <label for="room number">Room Number: </label>

        <%=f.number_field :room_number%>

        <%=f.submit "Create School class"%>

        <%end%>

